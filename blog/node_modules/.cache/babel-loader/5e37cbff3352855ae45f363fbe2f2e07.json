r;\n}\n .order .line {\n   display: inline-block;\n   width: 35%;\n   border-top: 1px solid #ccc ;\n}\n.order .txt {\n   color: #686868;\n   vertical-align: middle;\n   vertical-align: -8%;\n}\n.txt{\n  padding-left:10px;\n  padding-right:10px;\n  padding-top:10px;\n}\n\n</style>\n\n<template>\n  <div>\n    <el-button type=\"text\" id=\"zc\" @click=\"Register = true\">注册</el-button>\n\n    <el-dialog\n      title=\"注册流域\"\n      :modal-append-to-body=\"false\"\n      :visible.sync=\"Register\"\n      width=\"400px\"\n      center>\n\n      <span>\n        <el-form ref=\"regform\" :model=\"regform\" :rules=\"rules\" status-icon>\n          \n\n          <el-form-item prop=\"phone\" label=\"手机号\">\n            <el-input v-model=\"regform.phone\" placeholder=\"输入手机号，手机号将作为您的登录账号\"></el-input>\n          </el-form-item>\n\n\n          <el-form-item prop=\"yzm\" label=\"验证码\">\n            <el-input v-model=\"regform.yzm\" placeholder=\"输入验证码\">\n              <template slot=\"append\">发送验证码</template>\n            </el-input>\n          </el-form-item>\n\n          <el-form-item prop=\"password\" label=\"密码\">\n            <el-input v-model=\"regform.password\" type=\"password\" placeholder=\"输入密码\"></el-input>\n          </el-form-item>\n\n          <el-form-item prop=\"repassword\" label=\"确认密码\">\n            <el-input v-model=\"regform.repassword\" type=\"password\" placeholder=\"确认密码\"></el-input>\n          </el-form-item>\n\n          <el-form-item id=\"dl-none\">\n            <el-row>\n              <el-col :span=\"10\">\n                <div>已有账号？\n                  <a href=\"javascript:void(0)\" @click=\"dl()\" style=\"margin:0px;\">立即登录</a> \n                </div>\n              </el-col>\n            </el-row>\n          </el-form-item>\n\n          <el-form-item id=\"ds\">\n\n\n            <div class=\"order\">\n                <span class=\"line\"></span>\n                <span class=\"txt\">第三方登录</span>\n                <span class=\"line\"></span>\n    