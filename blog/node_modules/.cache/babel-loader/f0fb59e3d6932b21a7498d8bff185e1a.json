ch\"\n                size=\"medium\"\n                placeholder=\"搜索流域\"\n                suffix-icon=\"el-icon-search\"\n                v-model=\"input6\">\n      </el-input>\n    </div>\n\n    <div id=\"regLog\"\n         class=\"head-text\"\n         v-if=\"!verbState\">\n      <router-link to=\"/login\"\n                   @click.native=\"change('/login')\"\n                   :class=\"{'route' : route == '/login'}\"> 登录 </router-link>\n      <router-link to=\"/register\"\n                   @click.native=\"change('/register')\"\n                   :class=\"{'route' : route == '/register'}\"> 注册 </router-link>\n\n    </div>\n\n    <el-dropdown class=\"select\"\n                 @command=\"handleCommand\"\n                 v-if=\"verbState\">\n      <span class=\"el-dropdown-link\">\n        <avatar class=\"headImg\"\n                :src=\"user.avatar ? this.root+user.avatar :'' \"\n                :size=\"40\"\n                background-color=\"#1DA1F2\"\n                :username=\"user.name || '' \"></avatar>\n\n      </span>\n\n      <el-dropdown-menu slot=\"dropdown\">\n        <router-link class=\"user-link username\"\n                     :to=\"{ name: 'user', params: { id: user.id }}\">\n          <el-dropdown-item class=\"user-a\">\n            <i class=\"el-icon-info\"></i>\n            {{user.name}}\n            <div><i style=\"margin-right:5px;\"\n                 class=\"el-icon-mobile-phone\"></i>{{user.phone}}</div>\n\n          </el-dropdown-item>\n        </router-link>\n        <router-link class=\"user-link\"\n                     :to=\"{ name: 'user', params: { id: user.id }}\">\n          <el-dropdown-item class=\"user-a\">\n            <i class=\"el-icon-news\"></i>\n            个人中心\n          </el-dropdown-item>\n        </router-link>\n        <el-dropdown-item>\n          <i class=\"el-icon-bell\"></i>\n          通知消息</el-dropdown-item>\n        <el-dropdown-item>\n          <i class=\"el-icon-message\n\"></i>\n          私信消息</el-dropdown-item>\n\n        <router-link class=\"user-link\"\n                     :to=\"{ name: 'alterDetail', params: { id: user.id }}\">\n          <el-dropdown-item>\n            <i class=\"el-icon-setting\"></i>\n            编辑资料\n          </el-dropdown-item>\n        </router-link>\n\n        <el-dropdown-item command=\"logout\">\n          <i class=\"el-icon-circle-close-outline\"\n             style=\"margin-right:5px;\"></i>退出</el-dropdown-item>\n      </el-dropdown-menu>\n    </el-dropdown>\n    <el-dropdown class=\"publish\"\n                 v-if=\"verbState\">\n      <span class=\"el-dropdown-link\">\n        <i class=\"el-icon-arrow-down el-icon-circle-plus\n\"></i>\n      </span>\n\n      <el-dropdown-menu slot=\"dropdown\">\n        <router-link class=\"user-link\"\n                     :to=\"{ name: 'user', params: { id: user.id }}\">\n          <el-dropdown-item>\n            <i class=\"el-icon-edit\"></i> 发表文章\n          </el-dropdown-item>\n        </router-link>\n\n        <router-link class=\"user-link\"\n                     :to=\"{ name: 'user', params: { id: user.id }}\">\n          <el-dropdown-item>\n            <i class=\"el-icon-picture-outline\"></i> 发布照片</el-dropdown-item>\n        </router-link>\n\n      </el-dropdown-menu>\n    </el-dropdown>\n  </div>\n\n</template>\n\n<script>\nimport { mapState, mapMutations } from 'vuex'\nexport default {\n  data () {\n    return {\n      msg: '',\n      status: true,\n      search: '',\n      route: '',\n      input6: '',\n      root: '',\n    }\n  },\n  computed: {\n    ...mapState([\n      'verbState',\n      'user',\n    ]),\n\n  },\n  mounted: function () {\n    // 根据路径变更颜色\n    setTimeout(() => { this.route = this.$route.path }, 100);\n    this.root = this.$URL;\n  },\n  methods: {\n    ...mapMutations({\n      setverbState: 'SET_VERBSTATE',\n      setToken: 'SET_TOKEN',\n    }),\n    logstatus: function (logstatus) {\n      this.status = logstatus;\n    },\n    //变更颜色根据路径\n    change: function (route) {\n      this.route = route;\n    },\n    handleCommand: function (command) {\n      if (command === 'logout') this.logout();\n    },\n    logout: function () {\n      localStorage.removeItem('access_token');\n      localStorage.removeItem('verbState');\n      localStorage.removeItem('relogin');\n      //把登录后的token 存储在vuex 中\n      this.setverbState(false);\n      this.setToken('');\n\n      this.$message({\n        message: '已经成功退出',\n        type: 'warning'\n      });\n\n      this.$router.push({ name: 'login' });\n\n    },\n  }\n}\n</script>"],"sourceRoot":"src/components"}]}