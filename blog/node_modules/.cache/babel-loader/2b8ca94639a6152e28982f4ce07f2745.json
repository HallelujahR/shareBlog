el-icon-search\"\n                v-model=\"input6\">\n      </el-input>\n    </div>\n\n    <div class=\"head-text\"\n         v-if=\"!verbState\">\n      <router-link to=\"/login\"\n                   @click.native=\"changeColor($event, '0')\"> 登录 </router-link>\n      <router-link to=\"/register\"\n                   @click.native=\"changeColor($event, '0')\"> 注册 </router-link>\n    </div>\n\n    <el-dropdown v-else>\n      <span class=\"el-dropdown-link\">\n        <img class=\"headImg\"\n             :src=user.avatar\n             alt=\"\"\n             width=\"40px;\"\n             v-if=\"user.avatar\">\n        <avatar class=\"headImg\"\n                :size=\"40\"\n                :username=\"user.name || '' \"\n                v-else></avatar>\n      </span>\n      <el-dropdown-menu slot=\"dropdown\">\n        <el-dropdown-item>\n          <router-link :to=\"{ name: 'user', params: { id: user.id }}\"\n                       style=\"margin-right:10px;\">{{user.name}}</router-link>\n        </el-dropdown-item>\n        <el-dropdown-item>狮子头</el-dropdown-item>\n        <el-dropdown-item>螺蛳粉</el-dropdown-item>\n        <el-dropdown-item>双皮奶</el-dropdown-item>\n        <el-dropdown-item>蚵仔煎</el-dropdown-item>\n      </el-dropdown-menu>\n    </el-dropdown>\n  </div>\n\n</template>\n\n<script>\nimport { mapState, } from 'vuex'\n\nexport default {\n  data () {\n    return {\n      msg: '',\n      status: true,\n      search: '',\n      route: '',\n      input6: '',\n    }\n  },\n  computed: {\n    ...mapState([\n      'verbState',\n      'user',\n    ]),\n\n  },\n  mounted: function () {\n\n  },\n  methods: {\n    logstatus: function (logstatus) {\n      this.status = logstatus;\n    },\n    changeColor: function () {\n\n      setTimeout(() => { this.route = this.$route.path }, 100);\n      // eslint-disable-next-line no-console\n      console.log(this.route);\n\n    }\n  }\n}\n</script>"],"sourceRoot":"src/components"}]}