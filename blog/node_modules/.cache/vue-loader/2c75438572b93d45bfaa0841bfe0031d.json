eft:10px;\n  padding-right:10px;\n  padding-top:10px;\n}\n\n</style>\n\n<template>\n  <div>\n    <el-button type=\"text\" id=\"dl\" @click=\"Login = true\">登录</el-button>\n\n    <el-dialog\n      title=\"登录\"\n      :modal-append-to-body=\"false\"\n      :visible.sync=\"Login\"\n      width=\"30%\"\n      center>\n\n      <span>\n        <el-form ref=\"form\" :model=\"form\" :rules=\"rules\">\n          <el-form-item prop=\"name\" label=\"用户名\">\n            <el-input v-model=\"form.name\" placeholder=\"输入账号\"></el-input>\n          </el-form-item>\n          \n          <el-form-item prop=\"password\" label=\"密码\">\n            <el-input v-model=\"form.password\" type=\"password\" placeholder=\"输入密码\"></el-input>\n          </el-form-item>\n\n          <el-form-item id=\"dl-none\">\n            <el-row>\n              <el-col :span=\"10\">\n                <div>还没有账号？\n                  <a href=\"javascript:void(0)\" @click=\"reg()\" style=\"margin:0px;\">立即注册</a> \n                </div>\n              </el-col>\n\n              <el-col :span=\"4\" :offset=\"10\">\n                  <div>\n                    <a href=\"javascript:void(0)\" style='margin:0px;'>找回密码</a>\n                  </div>\n              </el-col>\n            </el-row>\n          </el-form-item>\n\n          <el-form-item id=\"ds\">\n\n\n            <div class=\"order\">\n                <span class=\"line\"></span>\n                <span class=\"txt\">第三方登录</span>\n                <span class=\"line\"></span>\n            </div>\n\n            <div style=\"text-align:center;float:left;width:100%\">\n              <img src=\"../../assets/weibo.png\" width=\"20px\" style=\"margin-left:-18px;\" >\n              <img src=\"../../assets/qq.png\" width=\"20px\" style=\"margin-left:30px\">\n            </div>\n          </el-form-item>\n\n\n        </el-form>\n      </span>\n\n      <span slot=\"footer\" class=\"dialog-footer\" >\n        <el-button id=\"qxdl\" @click=\"Login = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"dl('form')\">登 录</el-button>\n      </span>\n    </el-dialog>\n  </div> \n</template>\n<script>\n  export default {\n    data() {\n      return {\n        Login: false,\n        Register: false,\n        form: {\n          name: '',\n          password:'',\n        },\n        rules: {\n          name: [\n            { required: true, message: '请输入账号', trigger: 'blur' },\n            { min: 5, max: 20, message: '长度在 5 到 20 个字符', trigger: 'blur' }\n          ],\n          password:[\n            { required: true, message: '请输入密码', trigger: 'blur'},\n          ]\n        }\n      };\n\n    },\n    methods : {\n      reg:function(){\n\n        this.Login = false;\n        document.getElementById('zc').click();\n\n      },\n      dl:function(formName){\n\n        var _this = this;\n\n        this.$refs[formName].validate((valid) => {\n\n          if (valid) {\n\n                axios.post('http://api.blog.com/api/test',{\n\n              })\n              .then((res) => {\n\n                _this.$emit('logstatus', true)\n\n              })\n              .catch((error) => {\n\n              })\n            \n          } else {\n\n            return false;\n\n          }\n\n        });\n\n      }\n    }\n  };\n</script>"]}]}