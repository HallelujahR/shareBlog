eft:0px;\n    width:300px;\n    height:200px;\n  }\n</style>\n<template>\n  <div id=\"app\">\n    <div id=\"app-son\">\n    </div>\n\n    <transition name=\"el-fade-in\">\n\n      <div v-show=\"show\" class=\"transition-box app-main\">\n        <el-container >\n          <el-aside>\n            <dh></dh>\n          </el-aside>\n\n          <el-container>\n            <el-header id=\"app-header\">\n              <state-board></state-board>\n            </el-header>\n\n            <el-main id=\"main\"  style=\"margin-top:40px;border:1px solid red\">\n              <router-view :key=\"key\"></router-view>\n            </el-main>\n          </el-container>\n        </el-container>\n      </div>\n\n    </transition>\n\n  </div>\n</template>\n\n<script>\n    import { mapMutations, mapActions } from 'vuex'\n\n    export default {\n\n        data () {\n            return {\n                qwer:1,\n                msg:'as',\n                show:false,\n            }\n        },\n        components:{\n            'state-board': () => import('./components/loginOrRegister/stateBoard'),//引入上方导航栏组件\n            'dh': () => import ('./components/nav.vue'),//引入左侧导航组件\n        },\n        computed:{\n            key(){\n                return this.$route.path + Math.random();\n            }\n        },\n        methods:{\n            ...mapMutations({\n                setverbState: 'SET_VERBSTATE',\n                setToken: 'SET_TOKEN',\n            }),\n            ...mapActions([\n                'setUser',\n            ]),\n        },\n        created:function(){\n\n            //判断登录的状态\n            const verbState = localStorage.getItem('verbState');\n            const token = localStorage.getItem('access_token');\n\n            if(verbState === 'true'){\n\n                this.setverbState(true);\n                this.setToken(token);\n                this.setUser();\n\n            }else{\n                this.setverbState(false);\n                this.setToken('');\n            }\n        },\n        mounted:function(){\n\n            setTimeout(()=>{    this.show = true; } , 100);\n\n        },\n\n\n    }\n</script>\n<style>\n  #app {\n    font-family: 'Avenir', Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    text-align: center;\n    color: #2c3e50;\n  }\n</style>\n"]}]}